{
  "name": "vue-dynamic-mount",
  "version": "0.0.1",
  "description": "dynamically mount view by passing props to function",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "dev": "vite build --watch",
    "test": "vite",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint ./src --ext .vue,.js,.jsx,.cjs,.ts --fix",
    "nodemon": "nodemon -e js,ts,vue --watch src --exec \"yarn build\""
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "pinia": "^2.0.28",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.5",
    "vite": "^4.0.0",
    "vite-plugin-checker": "^0.6.0",
    "vue-tsc": "^1.0.12"
  }
}
